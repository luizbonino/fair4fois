#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/accrualPeriodicity
dct:accrualPeriodicity rdf:type owl:AnnotationProperty ;
                       rdfs:range dct:Frequency .


###  http://purl.org/dc/terms/alternative
dct:alternative rdf:type owl:AnnotationProperty ;
                rdfs:comment "Determines an alternative title for the resource. Accepts xsd:string and rdf:langString literals. E.g., 'OntoUML/UFO Catalog'@en."@en .


###  http://purl.org/dc/terms/conformsTo
dct:conformsTo rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
dct:contributor rdf:type owl:AnnotationProperty ;
                rdfs:comment "Identifies a foaf:Agent who contributed to the development of the resource."@en .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty ;
                rdfs:comment "Determines a free-text account of the resource. Accepts xsd:string and rdf:langString literals."@en .


###  http://purl.org/dc/terms/format
dct:format rdf:type owl:AnnotationProperty ;
           rdfs:comment "Identifies the format of the distribution. This property should be used to complement dcat:mediaType when the distribution format is not listed by IANA. We limit the use of this property with URIs so that more context about how to manipulate a distribution can be provided. E.g., https://www.file-extension.info/format/vpp."@en .


###  http://purl.org/dc/terms/hasVersion
dct:hasVersion rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dct:issued rdf:type owl:AnnotationProperty ;
           rdfs:comment "Determines when the resource was created. Accepts literals of the types xsd:dateTime, xsd:date, xsd:gYearMonth, and xsd:gYear. E.g., \"2018\"string^string^xsd:gYear, \"2018-01-15\"string^string^xsd:date. When cataloging a model from documents, we recommend using the publication date from the first one."@en .


###  http://purl.org/dc/terms/language
dct:language rdf:type owl:AnnotationProperty ;
             rdfs:comment "Determines a language in which the lexical labels of the semantic artefact are written. We require the use of values listed in the IANA Language Sub Tag Registry. E.g., 'en', 'pt'."@en .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty ;
            rdfs:comment "Identifies a legal document under which the resource is made available. E.g., https://creativecommons.org/licenses/by/4.0/."@en .


###  http://purl.org/dc/terms/modified
dct:modified rdf:type owl:AnnotationProperty ;
             rdfs:comment "Determines when the resource was last modified. Accepts literals of the types xsd:dateTime, xsd:date, xsd:gYearMonth, and xsd:gYear. When cataloging a model based on documents, we recommend using the publication date from the latest one."@en .


###  http://purl.org/dc/terms/temporal
dct:temporal rdf:type owl:AnnotationProperty ;
             rdfs:range dct:PeriodOfTime .


###  http://purl.org/dc/terms/temporalResolution
dct:temporalResolution rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
dct:title rdf:type owl:AnnotationProperty ;
          rdfs:comment "Determines a title for the resource. Accepts xsd:string and rdf:langString literals. E.g., \"Common Ontology of Value and Risk\"@en, \"FAIR Model Catalog for Ontology-Driven Conceptual Modeling Research\"@en. There must be at most one title per language."@en .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
vann:preferredNamespacePrefix rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
vann:preferredNamespaceUri rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/dcat#accessURL
dcat:accessURL rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/dcat#downloadURL
dcat:downloadURL rdf:type owl:AnnotationProperty ;
                 rdfs:comment "Identifies a URL that can be used to download the distribution."@en .


###  http://www.w3.org/ns/dcat#endDate
dcat:endDate rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/dcat#keyword
dcat:keyword rdf:type owl:AnnotationProperty ;
             rdfs:comment "Determines a domain (partially) described by the semantic artefact. Accepts xsd:string and rdf:langString literals. E.g., the User Feedback Ontology is described with the keywords 'online user feedback', 'software engineering', and 'requirements engineering'."@en .


###  http://www.w3.org/ns/dcat#mediaType
dcat:mediaType rdf:type owl:AnnotationProperty ;
               rdfs:comment "Identifies the media type of the distribution. Valid media types are only those defined by IANA, such as https://www.iana.org/assignments/media-types/application/json. If there is no media type for the format of the distribution, the type application/octet-stream must be used for binary files and the type text/plain must be used for text files."@en .


###  http://www.w3.org/ns/dcat#startDate
dcat:startDate rdf:type owl:AnnotationProperty .


###  https://w3id.org/mod#acronym
mod:acronym rdf:type owl:AnnotationProperty ;
            rdfs:comment "Determines an acronym one can use to refer to the semantic artefact. Accepts only xsd:string literals. E.g., 'RDBS-O', 'COVER', 'ROT'."@en .


###  https://w3id.org/mod#artefactType
mod:artefactType rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://purl.org/dc/terms/accrualPeriodicity
dct:accrualPeriodicity rdf:type owl:ObjectProperty ;
                       rdfs:domain dcat:Dataset .


###  http://purl.org/dc/terms/temporal
dct:temporal rdf:type owl:ObjectProperty ;
             rdfs:domain dcat:Dataset .


###  https://w3id.org/mod#designedForTask
mod:designedForTask rdf:type owl:ObjectProperty ;
                    rdfs:domain mod:SemanticArtefact ;
                    rdfs:range ocmv:OntologyDevelopmentContext ;
                    rdfs:comment "Identifies a goal that motivated the development of the semantic artefact."@en .


###  https://w3id.org/ontouml-models/vocabulary#conformsToSchema
ocmv:conformsToSchema rdf:type owl:ObjectProperty ;
                      rdfs:domain dcat:Distribution ;
                      rdfs:comment "Identifies a schema upon which the distribution can be validated against. E.g., a JSON Schema document, a SHACL shape, and an XML Schema document."@en ;
                      rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                      rdfs:label "conforms to schema"@en ;
                      skos:scopeNote "The identified schema should be compatible with the media type of the distribution. That is, if a distribution is in JSON, the schema cannot be an XML Schema."@en .


###  https://w3id.org/ontouml-models/vocabulary#context
ocmv:context rdf:type owl:ObjectProperty ;
             rdfs:domain mod:SemanticArtefact ;
             rdfs:range ocmv:OntologyDevelopmentContext ;
             rdfs:comment "Identifies an ocmv:Context in which the artefact was developed."@en ;
             rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
             rdfs:label "context"@en .


###  https://w3id.org/ontouml-models/vocabulary#ontologyType
ocmv:ontologyType rdf:type owl:ObjectProperty ;
                  rdfs:domain mod:SemanticArtefact ;
                  rdfs:range ocmv:OntologyType .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/ontouml-models/vocabulary#isComplete
ocmv:isComplete rdf:type owl:DatatypeProperty ;
                rdfs:domain dcat:Distribution ;
                rdfs:range xsd:boolean ;
                rdfs:comment "Determines if a distribution contains all the data from the dcat:Dataset it materializes. In the catalog, the distributions of models in VPP, JSON, and Turtle are complete, while those in image format are not."@en ;
                rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                rdfs:label "is complete"@en .


###  https://w3id.org/ontouml-models/vocabulary#storageUrl
ocmv:storageUrl rdf:type owl:DatatypeProperty ;
                rdfs:domain dcat:Dataset ;
                rdfs:range xsd:anyURI ;
                rdfs:comment "Determines a URL of a service in which the data and metadata of the dataset are stored. Accepts values in xsd:anyURI."@en ;
                rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                rdfs:label "storage URL"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.org/dc/terms/Frequency
dct:Frequency rdf:type owl:Class .


###  http://purl.org/dc/terms/PeriodOfTime
dct:PeriodOfTime rdf:type owl:Class .


###  http://www.w3.org/2002/07/owl#Ontology
owl:Ontology rdf:type owl:Class ;
             rdfs:subClassOf mod:SemanticArtefact .


###  http://www.w3.org/ns/dcat#Dataset
dcat:Dataset rdf:type owl:Class ;
             rdfs:subClassOf dcat:Resource .


###  http://www.w3.org/ns/dcat#Distribution
dcat:Distribution rdf:type owl:Class ;
                  rdfs:subClassOf dcat:Resource .


###  http://www.w3.org/ns/dcat#Resource
dcat:Resource rdf:type owl:Class .


###  https://w3id.org/FAIR-academic/fa-ont#ResearchResource
:ResearchResource rdf:type owl:Class ;
                  rdfs:subClassOf dcat:Resource .


###  https://w3id.org/mod#SemanticArtefact
mod:SemanticArtefact rdf:type owl:Class ;
                     rdfs:subClassOf dcat:Dataset .


###  https://w3id.org/mod#Taxonomy
mod:Taxonomy rdf:type owl:Class ;
             rdfs:subClassOf mod:SemanticArtefact .


###  https://w3id.org/mod#Terminology
mod:Terminology rdf:type owl:Class ;
                rdfs:subClassOf mod:SemanticArtefact .


###  https://w3id.org/mod#Thesaurus
mod:Thesaurus rdf:type owl:Class ;
              rdfs:subClassOf mod:SemanticArtefact .


###  https://w3id.org/ontouml-models/vocabulary#OntologyDevelopmentContext
ocmv:OntologyDevelopmentContext rdf:type owl:Class ;
                                rdfs:comment "Determines what is the context in which the artifact was developed."@en ;
                                rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                                rdfs:label "Ontology Development Context"@en .


###  https://w3id.org/ontouml-models/vocabulary#OntologyPurpose
ocmv:OntologyPurpose rdf:type owl:Class ;
                     rdfs:comment "Determines what is the main purpose of the artifact (e.g., conceptual clarification or data integration)."@en ;
                     rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                     rdfs:label "Ontology Purpose"@en .


###  https://w3id.org/ontouml-models/vocabulary#OntologyType
ocmv:OntologyType rdf:type owl:Class ;
                  rdfs:comment "Identifies the categorization of the artifacts according to their scopes."@en ;
                  rdfs:isDefinedBy <https://w3id.org/ontouml-models/vocabulary> ;
                  rdfs:label "Ontology Type"@en .


###  https://w3id.org/ontouml-models/vocabulary#SemanticArtefactDistribution
ocmv:SemanticArtefactDistribution rdf:type owl:Class ;
                                  rdfs:subClassOf dcat:Distribution .


